{
    "name": "hyl14s/mi-colegio",
    "description": "Pagina de venta de utiles escolares",
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-gd": "*",
        "ext-zip": "*",
        "dompdf/dompdf": "*",
        "phpoffice/phpspreadsheet": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
